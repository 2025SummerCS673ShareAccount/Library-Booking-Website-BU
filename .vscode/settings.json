{
    "cSpell.words": [
        "gtype",
        "jsonify",
        "libcal",
        "Referer",
        "supabase"
    ],
    
    // Disable Java and unnecessary extensions for this workspace
    "extensions.ignoreRecommendations": true,
    
    // Disable Java-related functionality
    "java.enabled": false,
    "java.configuration.detectJdksAtStart": false,
    "java.import.gradle.enabled": false,
    "java.server.launchMode": "Standard",
    "java.compile.nullAnalysis.mode": "disabled",
    
    // Disable specific extensions in this workspace
    "extensions.disabled": [
        // Java Extensions
        "redhat.java",
        "vscjava.vscode-java-debug", 
        "vscjava.vscode-maven",
        "vscjava.vscode-java-test",
        "vscjava.vscode-java-dependency",
        "vscjava.vscode-java-pack",
        "vscjava.vscode-gradle",
        "oracle.oracle-java",
        
        // C/C++ Extensions (if not needed for this project)
        "ms-vscode.cpptools-extension-pack",
        
        // Optional: IntelliCode API Usage Examples
        "visualstudioexptteam.intellicode-api-usage-examples"
    ],
    
    // File associations
    "files.associations": {
        "*.sql": "sql",
        "*.js": "javascript",
        "*.jsx": "javascriptreact", 
        "*.ts": "typescript",
        "*.tsx": "typescriptreact",
        "*.css": "css",
        "*.scss": "scss",
        "*.html": "html",
        "*.json": "json",
        "*.md": "markdown",
        "*.py": "python"
    },
    
    // SQL settings
    "sql.lint.enable": false,
    "sqltools.disableRollbar": true,
    "sql.validate": {
        "enable": false
    },
    "[sql]": {
        "editor.defaultFormatter": null,
        "editor.formatOnSave": false
    },
    
    // TypeScript settings
    "typescript.validate.enable": true,
    "typescript.preferences.includePackageJsonAutoImports": "auto",
    "typescript.preferences.quoteStyle": "single",
    "typescript.suggest.autoImports": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    
    // JavaScript settings
    "javascript.suggest.autoImports": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    
    // Editor settings
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.formatOnSave": true,
    
    // Language-specific formatters (using built-in formatters)
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    
    // ESLint configuration
    "eslint.workingDirectories": ["admin-page", "bu-book"],
    "eslint.validate": [
        "javascript",
        "javascriptreact", 
        "typescript",
        "typescriptreact"
    ],
    
    // Search and file exclusions
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "database_schema.sql": true,
        "database_schema_simple.sql": true,
        // Java-related exclusions
        "**/.gradle": true,
        "**/.m2": true,
        "**/target": true,
        "**/build": true,
        "**/*.class": true,
        "**/*.jar": true
    },
    
    "files.exclude": {
        "database_schema.sql": false,
        // Java-related file exclusions
        "**/.gradle": true,
        "**/.m2": true,
        "**/target": true,
        "**/build": true,
        "**/*.class": true,
        "**/*.jar": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    
    "files.watcherExclude": {
        "database_schema.sql": true,
        // Java-related watcher exclusions
        "**/.gradle/**": true,
        "**/.m2/**": true,
        "**/target/**": true,
        "**/build/**": true,
        "**/*.class": true,
        "**/*.jar": true
    },
    
    // Performance optimizations
    "problems.decorations.enabled": true,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "telemetry.telemetryLevel": "off",
    
    // Terminal configuration
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        }
    }
}